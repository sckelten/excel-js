{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "linebreak-style": "off",
    "require-jsdoc": "off",
    "operator-linebreak": "off",
    "no-unused-vars": "warn",
  },
  "extends": ["eslint:recommended", "google"]
}